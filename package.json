{
  "name": "timer_app_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev_env": "sst bind next dev --stage development",
    "start": "sst bind next start --stage development",
    "start_dev": "set TEST_ENV=true && sst bind next dev --stage development",
    "dev": "next dev",
    "build_test": "next build",
    "build": "sst bind next build",
    "__deploy": "serverless",
    "deploy": "npx sst deploy",
    "sst": "npx sst",
    "lint": "eslint . --max-warnings 0 --ext .js,.jsx",
    "lintfix": "eslint . --max-warnings 0 --ext .js,.jsx,.ts,.tsx --fix",
    "test": "playwright test",
    "test-ci": "start-server-and-test start_dev http://localhost:3000 test",
    "__test-ci": "start-test start_dev 3000 test ",
    "test-browser": "playwright test --project=chromium",
    "generate": "playwright codegen --viewport-size \"1920, 2000\""
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.6.7",
    "classnames": "^2.3.2",
    "cookie": "^0.6.0",
    "dateformat": "^5.0.3",
    "fast-json": "^3.0.0",
    "jose": "^5.2.3",
    "js-cookie": "^3.0.5",
    "next": "14.0.1",
    "prettier": "^3.2.2",
    "react": "^18",
    "react-color": "^2.19.3",
    "react-dom": "^18",
    "react-modal": "^3.16.1",
    "react-popper": "^2.3.0",
    "react-toastify": "^10.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/cookie": "^0.6.0",
    "@types/dateformat": "^5.0.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.11.2",
    "@types/react": "^18.2.48",
    "@types/react-color": "^3.0.12",
    "@types/react-dom": "^18.2.18",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.19",
    "constructs": "10.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
